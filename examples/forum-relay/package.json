{
  "private": true,
  "name": "postgraphql-example-forum-relay",
  "version": "1.0.0",
  "description": "A PostGraphQL forum example with a Relay frontend.",
  "scripts": {
    "schema-up": "psql -f ./schema.sql",
    "schema-down": "psql -c 'drop schema if exists forum_example, forum_example_utils cascade'",
    "schema": "npm run schema-down && npm run schema-up",
    "start": "babel-node ./server.js",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-relay-plugin": "^0.9.2",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "fs-promise": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.1.7",
    "pg": "^5.1.0",
    "postgraphql": "^1.8.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-relay": "^0.9.2",
    "react-router": "^2.6.1",
    "react-router-relay": "^0.13.3",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
